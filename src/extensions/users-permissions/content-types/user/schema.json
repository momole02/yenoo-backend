{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "enterpriseName": {
      "type": "string"
    },
    "job": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "is2FAEnabled": {
      "type": "boolean"
    },
    "account_keychains": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account-keychain.account-keychain",
      "mappedBy": "owner"
    },
    "licence_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::licence-subscription.licence-subscription",
      "mappedBy": "owner"
    },
    "service_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-subscription.service-subscription",
      "mappedBy": "owner"
    },
    "slug": {
      "type": "uid",
      "targetField": "firstName"
    },
    "twoFactorAuthCode": {
      "type": "string"
    },
    "pwd": {
      "type": "string"
    },
    "secure_link_accesses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::secure-link-access.secure-link-access",
      "mappedBy": "user"
    }
  }
}
